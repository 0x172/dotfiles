# Unix
alias tlf="tail -f"
alias ln='ln -v'
alias mkdir='mkdir -p'
alias ...='../..'
alias l='ls'
alias ll='ls -al'
alias lh='ls -Alh'
alias -g G='| grep'
alias -g M='| less'
alias -g L='| wc -l'
alias -g ONE="| awk '{ print \$1}'"
alias e="$EDITOR"
alias v="$VISUAL"

# git

# Pretty print the path
alias path="echo $PATH | tr -s ':' '\n'"

# Navigation
alias cdproj="cd ~/projects"
alias cdwiki="cd ~/Dropbox/vimwiki"
alias cd='cs'
alias bsync="browser-sync start --proxy localhost:3000"
# This kills a process that slows down my Mac every once in a while...??
alias killn="ps -ax | grep notifyd | head -1 | awk '{print \$1}' | sudo xargs kill"
